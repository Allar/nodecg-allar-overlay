{
  "name": "nodecg-allar-overlay",
  "version": "0.1.0",
  "description": "Dynamic broadcast graphics rendered in a browser",
  "homepage": "http://allarsblog.com",
  "author": {
    "name": "Michael Allar",
    "email": "allar@michaelallar.com",
    "url": "http://allarsblog.com"
  },
  "license": "MIT",
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "allar",
    "nodecg",
    "background",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~0.8.0",
    "dashboardPanels": [
      {
        "name": "background",
        "title": "Background",
        "width": 2,
        "file": "background.html",
        "headerColor": "#9f9bbd"
      }
    ],
    "graphics": [
      {
        "file": "background.html",
        "width": 1920,
        "height": 1080,
        "singleInstance": false
      }
    ]
  },
  "repository": "https://github.com/Allar/nodecg-allar-overlay"
}
